{
  "expo": {
    "name": "Medi Assist Bot", 
    "slug": "medi-assist-bot",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediassist.bot"
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA"
      ],
      "package": "com.mediassist.bot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
